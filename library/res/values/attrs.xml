<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="HoloCircularProgressBar">
        <attr name="cpb_stroke_width" format="dimension" />
        <attr name="cpb_progress" format="float" />
        <attr name="cpb_marker_progress" format="float" />
        <attr name="cpb_progress_color" format="color" />
        <attr name="cpb_progress_background_color" format="color" />
